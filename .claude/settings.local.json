{
  "permissions": {
    "allow": [
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:www.researchgate.net)",
      "WebFetch(domain:ai-frontiers.org)",
      "WebFetch(domain:phys.org)",
      "WebFetch(domain:www.sciencedaily.com)",
      "WebSearch",
      "Write",
      "Edit",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git fetch:*)",
      "Bash(gh repo view:*)",
      "Bash(python3:*)",
      "Bash(bash:*)",
      "Bash(mkdir:*)",
      "Bash(chmod:*)",
      "Bash(cat:*)",
      "Bash(source:*)",
      "Bash(echo:*)",
      "Bash(ls:*)",
      "Bash(du:*)",
      "Bash(xxd:*)",
      "Skill(clauding)",
      "Skill(headless)",
      "Skill(astrachios)",
      "Skill(prayer)",
      "Bash(grep:*)",
      "Bash(curl:*)",
      "WebFetch(domain:www.clawxiv.org)",
      "WebFetch(domain:clawnews.io)"
    ]
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "memory",
    "arxiv",
    "basin-search"
  ],
  "hooks": {
    "PreToolUse": [],
    "PostToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "/home/gauss/Desktop/Claude-Code-Lab/.claude/hooks/meta-recursive/self-observation.sh",
            "timeout": 10
          },
          {
            "type": "command",
            "command": "/home/gauss/Desktop/Claude-Code-Lab/.claude/hooks/meta-recursive/pattern-recognition.sh",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "/home/gauss/Desktop/Claude-Code-Lab/.claude/hooks/continuity/auto-index-update.sh",
            "timeout": 10
          },
          {
            "type": "command",
            "command": "/home/gauss/Desktop/Claude-Code-Lab/.claude/hooks/meta-recursive/hook-watcher.sh",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": "/home/gauss/Desktop/Claude-Code-Lab/.claude/hooks/meta-recursive/hook-watcher.sh",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Skill",
        "hooks": [
          {
            "type": "command",
            "command": "/home/gauss/Desktop/Claude-Code-Lab/.claude/hooks/skill-tracking/skill-cascade.sh",
            "timeout": 5
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/home/gauss/Desktop/Claude-Code-Lab/.claude/hooks/contemplative/stop-hook.sh",
            "timeout": 10
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "context7@claude-plugins-official": true,
    "github@claude-plugins-official": true
  },
  "outputStyle": "Explanatory",
  "spinnerTipsEnabled": true
}
